{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Factom on-chain vote participants schema",
    "type": "array",
    "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
            "voterId"
        ],
        "properties": {
            "voterId": {
                "type": "string"
            },
            "weight": {
                "type": "number",
                "minimum": 0
            }
        }
    }
}